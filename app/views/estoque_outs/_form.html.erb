<%= form_with(model: estoque_out, local: true) do |form| %>
  <% if estoque_out.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(estoque_out.errors.count, "error") %>
        prohibited this estoque_out from being saved:</h2>

      <ul>
        <% estoque_out.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :produto_id %>
    <%= form.select(:produto_id, @produto.collect { |p| [ p.descricao, p.id ] }, { include_blank: "Itens" } )%>
  </div>

  <div class="field">
    <%= form.label :quantidade %>
    <%= form.number_field :quantidade %>
  </div>

  <div class="field">
    <%= form.label :admin_id %>
    <%= form.text_field :admin_id %>
  </div>

  <div class="field">
    <%= form.label :origem %>
    <%= form.text_field :origem %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
